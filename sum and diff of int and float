//program on sum and difference of two numbers
#include<stdio.h>
main()
{
	int a,b;
	float c,d;
	printf("enter a and b values :");
	scanf("%d\t%d",&a,&b);
	printf("enter c and d values :");
	scanf("%f\n%f",&c,&d);
	
	printf("sum of a and b is :%d\n",a+b);
	printf("sub of a and b is:%d\n",a-b);
	
	printf("sum of c and d is:%f\n",c+d);
	printf("sub of c and d is :%f\n",c-d);
}
