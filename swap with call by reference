#include<stdio.h>
void swap(int *x,int *y);
main()
{
	int a,b;
	printf("enter a and b values:");
	scanf("%d %d",&a,&b);
	swap(&a,&b);
	printf("in main function after swapping is  a=%d ,b=%d:",a,b);
	
}
void swap(int *x,int *y)
{
	int temp;
	temp=*x;
	*x=*y;
	*y=temp;
	printf("after swapping a=%d ,b=%d\n",*x,*y);
}
