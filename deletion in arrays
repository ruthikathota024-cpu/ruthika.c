#include<stdio.h>
int main()
{
	int i,n,a[10],pos;
	printf("enter n value:");
	scanf("%d",&n);
	printf("enter array elements:");
	for(i=0;i<=n;i++)
	{
	scanf("%d",&a[i]);
	}
	printf("enter psition:");
	scanf("%d",&pos);
	for(i=pos;i<n;i++)
	{
		a[i]=a[i+1];
	}
	for(i=0;i<n;i++)
	{
		printf("%d\t",a[i]);
	}
	
}
